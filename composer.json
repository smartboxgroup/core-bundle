{
    "name": "smartbox/core-bundle",
    "description": "Smartbox Core Bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
      "psr-4": {
        "Smartbox\\CoreBundle\\": ""
      }
    },
    "require": {
        "php": ">=7.1",
        "jms/serializer": "*",
        "symfony/symfony": "3.4.*",
        "symfony/console": "*",
        "symfony/framework-bundle": "*",
        "symfony/property-access": "*"
    },
    "require-dev": {
        "jms/serializer-bundle": "*",
        "predis/predis": "*",
        "snc/redis-bundle": "*",
        "symfony/monolog-bundle": "*",
        "symfony/phpunit-bridge": "*",
        "symfony/validator": "*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}
